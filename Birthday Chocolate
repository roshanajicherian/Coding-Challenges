#include <iostream>
using namespace std;

int main()
{
    int n=0,a[500],d=0,m=0,co=0;
    cin>>n;
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    cin>>d>>m;
    for(int i=0;i<=n-m;i++)
    {   int sum=0;
        for(int j=i;j<i+m;j++)
        {
            sum+=a[j];
        }
        if(sum==d)
        {
            co++;
            
        }
        
    }
    cout<<co;
return 0;
}

