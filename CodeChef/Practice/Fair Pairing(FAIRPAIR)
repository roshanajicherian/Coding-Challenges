#include <bits/stdc++.h>
typedef long long int lli;
using namespace std;
int main()
{
    lli t=0;
    cin>>t;
    while(t--)
    {
    lli n=0,loc=0,task=0,stu=0,pass=0;
    cin>>n;
    lli S[n+1],T[n+1],res[n+1];
    T[0]=-1;
    S[0]=INT_MAX;
    for(lli i=1;i<=n;i++)
        cin>>S[i];
    for(lli i=1;i<=n;i++)
        cin>>T[i];
    sort(S+1,S+n+1);
        for(lli i=1;i<=n;i++)
        {
            lli maxe=INT_MIN;
            stu=S[i];
            for(lli j=1;j<=n;j++)
            {
                if(T[j]>=maxe && T[j]!=-1)
                {
                    maxe=T[j];
                    loc=j;
                }
            }
            task=maxe;
            T[loc]=-1;
            if(stu==task)
            {
                pass++;
            }
            res[i]=loc;
        }
        cout<<pass<<'\n';
        for(lli i=1;i<=n;i++)
            cout<<res[i]<<' ';
        cout<<'\n';
    }
    return 0;
}
